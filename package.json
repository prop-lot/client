{
  "name": "prop-lot-client",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "generate-server-graph": "graphql-codegen --config codegen.yml",
    "generate-client-graph": "npx apollo codegen:generate --localSchemaFile=./graphql/schemas/schema.graphql ./graphql/types/__generated__/ --outputFlat --globalTypesFile=./graphql/types/__generated__/globalTypes.ts --includes=./graphql/queries/**/*.ts --target=typescript --tagName=gql"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@apollo/server": "^4.3.1",
    "@as-integrations/next": "^1.2.0",
    "@graphql-tools/schema": "^9.0.13",
    "@graphql-tools/utils": "^9.1.4",
    "@headlessui/react": "^1.7.8",
    "@heroicons/react": "^2.0.14",
    "@lilnounsdao/assets": "^0.5.9",
    "@lilnounsdao/sdk": "^0.3.10",
    "@next/font": "13.1.4",
    "@prisma/client": "^4.9.0",
    "@types/marked": "^4.0.8",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@usedapp/core": "^1.2.2",
    "bootstrap": "^5.2.3",
    "chance": "^1.1.9",
    "connectkit": "^1.1.1",
    "dompurify": "^2.4.3",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.4",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "graphql-import-node": "^0.0.5",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "history": "^5.3.0",
    "iron-session": "^6.3.1",
    "isomorphic-dompurify": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "marked": "^4.2.12",
    "moment": "^2.29.4",
    "next": "13.1.4",
    "p-retry": "^5.1.2",
    "react": "18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.7.0",
    "react-use": "^17.4.0",
    "siwe": "^2.1.3",
    "urql": "^3.0.3",
    "uuid": "^9.0.0",
    "wagmi": "^0.9.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-resolvers": "^2.7.12",
    "@tailwindcss/typography": "^0.5.9",
    "@types/chance": "^1.1.3",
    "@types/dompurify": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
